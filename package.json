{
  "name": "doar-computadores",
  "version": "1.0.0",
  "description": "Receber e retornar informações para uma interface com objetivo interligar doadores e donatário",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon ./src/main.js",
    "start": "npx typeorm migration:run -d ./src/database/ormconfig.js && node ./src/main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll",
    "migration:up": "typeorm migration:run -d ./src/database/ormconfig.js",
    "migration:down": "typeorm migration:revert -d ./src/database/ormconfig.js"
  },
  "author": "Leonardo Frederico da Silva",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "mysql2": "^2.3.3",
    "typeorm": "^0.3.7"
  }
}
